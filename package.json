{
  "name": "team-topic",
  "version": "1.0.0",
  "description": "This is a web application for show how to use graphql in react",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "prebuild": "npm run clean-dist && npm run clean-dist-stable && npm run lint",
    "build": "babel-node tools/build.js",
    "build-stable": "npm run prebuild && babel-node tools/buildStable.js",
    "generate": "babel-node tools/generateComponent/generateComponent.js"
  },
  "author": "CloudShadow.Me",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^2.2.0",
    "apollo-client-preset": "^1.0.6",
    "apollo-link-ws": "^1.0.4",
    "axios": "^0.17.1",
    "es6-promise": "4.1.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "history": "4.6.3",
    "object-assign": "4.1.0",
    "react": "15.4.1",
    "react-apollo": "^2.0.4",
    "react-dom": "15.4.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.16.0",
    "browser-sync": "2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "identity-obj-proxy": "3.0.0",
    "json-loader": "0.5.4",
    "node-sass": "4.0.0",
    "npm-run-all": "3.1.2",
